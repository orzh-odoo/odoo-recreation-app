        <div class="score-element" t-on-click.stop="">
            <span class="score-team-name">
                <t t-esc="props.element.teamName"/>
            </span>
            <span class="score-score">
                <t t-esc="props.element.score"/>
            </span>
        </div>
    </t>

</template>