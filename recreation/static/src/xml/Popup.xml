<?xml version="1.0" encoding="utf-8"?>
<template id="template" xml:space="preserve">

    <t t-name="popup" owl="1">
        <div class="modal fade" id="staticBackdrop" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="staticBackdropLabel" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="staticBackdropLabel">Game Over</h5>
                </div>
                <div class="modal-body">
                    <h1>
                        Epic Team is the Winner!
                    </h1>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  <button type="button" class="btn btn-warning">END GAME</button>
                  <button type="button" class="btn btn-primary">Rematch</button>
                </div>
              </div>
            </div>
          </div>
    </t>

</template>